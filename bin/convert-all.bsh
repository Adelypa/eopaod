#!/bin/bash

cd ~/git/eopaod/src

function convert_country {
  echo "Converting $1..."
  ruby psv2csv.rb $1
  ruby psv2svg.rb $1
}

convert_country at
ruby psv2csv.rb bg
ruby psv2csv.rb cy
convert_country cz
convert_country de
convert_country dk
ruby psv2csv.rb ee
convert_country fi
ruby psv2csv.rb hr
ruby psv2csv.rb lt
ruby psv2csv.rb nl
ruby psv2csv.rb pl
ruby psv2csv.rb ro
ruby psv2csv.rb sk
